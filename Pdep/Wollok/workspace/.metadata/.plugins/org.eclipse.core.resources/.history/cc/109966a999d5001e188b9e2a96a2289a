object pepita {
	var energia = 100
	
	method vola(kilometros){
		energia = energia - kilometros*2
	}
	
	method come(gramos){
		energia = energia + gramos*10
	}
	
	method energia() = energia
	
}

object ramiro {
	var dormido = 0
	method dormido() = dormido
	
	method dormir(horas){
		dormido = dormido + horas
	}
	 
	method entrena(ave){
		if (dormido < 8){
			ave.vola(15*2)
		}
		else {
			ave.vola(15)
			}
		}
}	

object emilia {
	method entrena(ave){
		ave.come(5)
		ave.vola(10)
		ave.come(5)
	}
}

object pepote {
	var volado = 0
	var comido = 0
	
	method vola(kilometros){
		volado = volado + kilometros
	}
	
	method come(gramos){
		comido = comido + gramos
	}
	
	method energia() = 255 + comido**2 - volado/5
}